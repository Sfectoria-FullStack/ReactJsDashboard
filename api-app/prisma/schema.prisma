generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id() @default(autoincrement())
  fullName String
  age      Int
  posts    Post[]
}

model Post {
  id          Int    @id() @default(autoincrement())
  imageURL    String
  productName String
  price       Int
  User        User  @relation(fields: [userId], references: [id])
  userId      Int
}
