generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Products {
  id          Int     @id @default(autoincrement())
  title       String  @unique
  description String?
  price       Int
  Users       Users?  @relation(fields: [usersId], references: [id])
  usersId     Int?
}

model Users {
  id       Int        @id @default(autoincrement())
  fullName String
  products Products[]
}
